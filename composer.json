{
    "name": "seregazhuk/react-memcached",
    "description": "Async Memcached client implementation, built on top of ReactPHP",
    "keywords": [
        "Memcached",
        "client",
        "async",
        "ReactPHP"
    ],
    "homepage": "https://github.com/seregazhuk/php-react-memcached",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Zhuk",
            "email": "seregazhuk88@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "react/socket": "^0.8",
        "react/promise": "^2.7",
        "react/event-loop": "^0.5",
        "evenement/evenement": "3"
    },
    "autoload": {
        "psr-4": {
            "seregazhuk\\React\\Memcached\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "seregazhuk\\React\\Memcached\\tests\\": "tests/"
        }
    },
    "require-dev": {
        "seregazhuk/react-promise-testing": "0.*",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~5.0",
        "codeclimate/php-test-reporter": "^0.4.4"
    }
}
